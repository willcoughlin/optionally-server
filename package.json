{
  "name": "optionally-server",
  "version": "1.0.0",
  "description": "OptionAlly GraphQL server",
  "main": "src/server.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc && copyfiles && copyfiles -u 2 src/graphql/*.graphql dist/graphql/",
    "start:dev": "nodemon \"src/server.ts\" --exec \"ts-node\" src/server.ts -e ts,graphql",
    "generate:graphql": "graphql-codegen --config graphql-codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willcoughlin/optionally-server.git"
  },
  "author": "Will Coughlin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/willcoughlin/optionally-server/issues"
  },
  "homepage": "https://github.com/willcoughlin/optionally-server#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.4",
    "@graphql-codegen/introspection": "1.17.4",
    "@graphql-codegen/typescript": "1.17.4",
    "@graphql-codegen/typescript-resolvers": "1.17.4",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.7",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.26",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-import-node": "0.0.4",
    "jest": "^26.4.0",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@graphql-codegen/add": "^1.17.4",
    "apollo-datasource-rest": "^0.9.3",
    "apollo-server-express": "^2.16.0",
    "black-scholes": "^1.1.0",
    "compression": "^1.7.4",
    "copyfiles": "^2.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "http": "0.0.1-security",
    "moment": "^2.27.0",
    "ncp": "^2.0.0"
  }
}
